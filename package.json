{
  "name": "mdx.org.ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:mdx": "vitest run --config vitest.mdx.config.ts",
    "lint": "turbo run lint",
    "lint:root": "eslint .",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "turbo run clean && rm -rf node_modules",
    "mdxai": "node packages/mdxai/dist/cli.js",
    "mdxe": "node packages/mdxe/dist/cli.js",
    "mdxe:dev": "node packages/mdxe/dist/cli.js dev --dir examples",
    "mdxe:test": "node packages/mdxe/dist/cli.js test",
    "notebook": "node packages/mdxe/dist/cli.js notebook examples/notebook.mdx --open"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:*",
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.0",
    "eslint": "^9.16.0",
    "hono": "^4.6.0",
    "ink": "^5.0.1",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20"
  }
}
