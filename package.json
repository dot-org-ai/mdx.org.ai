{
  "name": "mdx.org.ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:mdx": "vitest run --config vitest.mdx.config.ts",
    "lint": "turbo run lint",
    "lint:root": "eslint .",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "turbo run clean && rm -rf node_modules",
    "changeset": "changeset",
    "changeset:status": "changeset status",
    "version": "changeset version",
    "version:bump": "./scripts/bump-versions.sh",
    "version:check": "./scripts/check-versions.sh",
    "release": "./scripts/release.sh",
    "publish:check": "tsx scripts/publish-all.ts --check",
    "publish:dry": "tsx scripts/publish-all.ts --dry-run",
    "publish:all": "tsx scripts/publish-all.ts",
    "release:dry": "./scripts/release.sh --dry-run",
    "prerelease": "./scripts/prerelease.sh",
    "prerelease:exit": "changeset pre exit",
    "publish": "changeset publish",
    "mdxai": "node packages/mdxai/dist/cli.js",
    "mdxe": "node packages/mdxe/dist/cli.js",
    "mdxe:dev": "node packages/mdxe/dist/cli.js dev --dir examples",
    "mdxe:test": "node packages/mdxe/dist/cli.js test",
    "notebook": "node packages/mdxe/dist/cli.js notebook examples/notebook.mdx --open",
    "studio": "node packages/mdxdb/dist/cli.js studio --path examples",
    "storybook": "pnpm --filter storybook dev"
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.16.0",
    "@mdxld/config": "workspace:*",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.0",
    "ai-database": "workspace:*",
    "eslint": "^9.16.0",
    "hono": "^4.6.0",
    "ink": "^5.0.1",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "rpc.do": "^0.1.2",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20"
  }
}
