{
  "name": "@mdxui/magicui",
  "version": "0.0.0",
  "description": "Animated UI components - backgrounds, text effects, and interactive elements",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./backgrounds": {
      "types": "./dist/backgrounds/index.d.ts",
      "import": "./dist/backgrounds/index.js",
      "require": "./dist/backgrounds/index.cjs"
    },
    "./text": {
      "types": "./dist/text/index.d.ts",
      "import": "./dist/text/index.js",
      "require": "./dist/text/index.cjs"
    },
    "./effects": {
      "types": "./dist/effects/index.d.ts",
      "import": "./dist/effects/index.js",
      "require": "./dist/effects/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "ogl": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "ogl": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "react": "^18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "magicui",
    "animated",
    "backgrounds",
    "effects",
    "react",
    "components"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxui/magicui"
  },
  "author": "MDX.org.ai"
}
