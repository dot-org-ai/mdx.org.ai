{
  "name": "@mdxui/shadcnblocks",
  "version": "0.0.1",
  "description": "Shadcnblocks components mapped to MDXUI abstract Site/App types",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./blocks": {
      "types": "./dist/blocks/index.d.ts",
      "import": "./dist/blocks/index.js",
      "require": "./dist/blocks/index.cjs"
    },
    "./templates": {
      "types": "./dist/templates/index.d.ts",
      "import": "./dist/templates/index.js",
      "require": "./dist/templates/index.cjs"
    },
    "./templates/layouts": {
      "types": "./dist/templates/layouts/index.d.ts",
      "import": "./dist/templates/layouts/index.js",
      "require": "./dist/templates/layouts/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "shadcn",
    "shadcnblocks",
    "react",
    "components",
    "blocks",
    "mdxui"
  ],
  "author": "MDX.org.ai",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@mdxui/shadcn": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
