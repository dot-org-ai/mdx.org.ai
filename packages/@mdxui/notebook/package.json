{
  "name": "@mdxui/notebook",
  "version": "1.9.0",
  "description": "Jupyter Notebook-style components for MDX with browser and RPC execution",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./execution": {
      "import": "./dist/execution/index.js",
      "require": "./dist/execution/index.cjs",
      "types": "./dist/execution/index.d.ts"
    },
    "./outputs": {
      "import": "./dist/outputs/index.js",
      "require": "./dist/outputs/index.cjs",
      "types": "./dist/outputs/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.34.1",
    "@uiw/react-codemirror": "^4.23.6",
    "quickjs-emscripten": "^0.31.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6"
  },
  "keywords": [
    "notebook",
    "jupyter",
    "mdx",
    "react",
    "code-execution",
    "rpc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-org-ai/mdx.org.ai",
    "directory": "packages/@mdxui/notebook"
  },
  "license": "MIT",
  "author": "MDX.org.ai"
}
