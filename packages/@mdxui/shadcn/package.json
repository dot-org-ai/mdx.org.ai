{
  "name": "@mdxui/shadcn",
  "version": "1.9.0",
  "description": "Shadcn/ui components with tweakcn theme presets for MDX",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./themes": {
      "types": "./dist/themes.d.ts",
      "import": "./dist/themes.js",
      "require": "./dist/themes.cjs"
    },
    "./styles.css": "./src/styles.css"
  },
  "files": [
    "dist",
    "src/styles.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxui",
    "shadcn",
    "shadcn-ui",
    "tweakcn",
    "components",
    "ui",
    "react",
    "tailwind",
    "themes"
  ],
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@mdxui/cmdk": "workspace:^",
    "@mdxui/jsx": "workspace:^",
    "@mdxui/radix": "workspace:^",
    "@mdxui/sonner": "workspace:^",
    "@mdxui/vaul": "workspace:^",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.555.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.11.3",
    "react-hook-form": "^7.67.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "2.15.4",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^3.4.0 || ^4.0.0"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxui/shadcn"
  },
  "author": "MDX.org.ai"
}
