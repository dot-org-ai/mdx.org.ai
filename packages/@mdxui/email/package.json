{
  "name": "@mdxui/email",
  "version": "0.0.1",
  "description": "Render MDXLD documents to email HTML using React Email",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxui",
    "email",
    "react-email",
    "mdxld",
    "render"
  ],
  "license": "MIT",
  "dependencies": {
    "@react-email/components": "^0.0.31",
    "@react-email/render": "^1.0.3"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "mdxld": "workspace:^",
    "react": "^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxui/email"
  },
  "author": "MDX.org.ai"
}
