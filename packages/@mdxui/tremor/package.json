{
  "name": "@mdxui/tremor",
  "version": "2.0.0",
  "description": "Tremor-inspired dashboard components built with @mdxui/headless for Hono JSX compatibility",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./charts": {
      "types": "./src/charts/index.ts",
      "import": "./dist/charts/index.js",
      "require": "./dist/charts/index.cjs"
    },
    "./blocks": {
      "types": "./src/blocks/index.ts",
      "import": "./dist/blocks/index.js",
      "require": "./dist/blocks/index.cjs"
    },
    "./ui": {
      "types": "./src/ui/index.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs"
    },
    "./templates": {
      "types": "./src/templates/index.ts",
      "import": "./dist/templates/index.js",
      "require": "./dist/templates/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "tremor",
    "dashboard",
    "charts",
    "components",
    "mdxui",
    "hono",
    "headless"
  ],
  "author": "MDX.org.ai",
  "license": "MIT",
  "peerDependencies": {
    "hono": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "hono": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@mdxui/jsx": "workspace:^",
    "@mdxui/headless": "workspace:^"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxui/tremor"
  }
}
