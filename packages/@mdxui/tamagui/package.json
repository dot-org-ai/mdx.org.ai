{
  "name": "@mdxui/tamagui",
  "version": "1.9.0",
  "description": "Tamagui components for rendering MDX content - universal React Native & Web",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./themes": {
      "types": "./dist/themes.d.ts",
      "import": "./dist/themes.js",
      "require": "./dist/themes.cjs"
    },
    "./primitives": {
      "types": "./dist/primitives.d.ts",
      "import": "./dist/primitives.js",
      "require": "./dist/primitives.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxui",
    "tamagui",
    "react-native",
    "universal",
    "components",
    "ui",
    "mdx",
    "mdxld"
  ],
  "license": "MIT",
  "dependencies": {
    "mdxld": "workspace:*"
  },
  "peerDependencies": {
    "@tamagui/core": ">=1.100.0",
    "@tamagui/config": ">=1.100.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-native": ">=0.73.0",
    "tamagui": ">=1.100.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@tamagui/config": "^1.116.11",
    "@tamagui/core": "^1.116.11",
    "@types/react": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^18.3.1",
    "tamagui": "^1.116.11",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxui/tamagui"
  },
  "author": "MDX.org.ai"
}
