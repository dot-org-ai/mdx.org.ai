{
  "name": "@mdxui/examples",
  "version": "0.0.1",
  "description": "Example MDX sites and applications for @mdxui",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./sites/*": {
      "types": "./dist/sites/*.d.ts",
      "import": "./dist/sites/*.js",
      "require": "./dist/sites/*.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "content"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "compile": "node scripts/compile.js"
  },
  "keywords": [
    "mdxui",
    "examples",
    "mdx",
    "sites",
    "templates"
  ],
  "author": "MDX.org.ai",
  "license": "MIT",
  "dependencies": {
    "@mdxui/shadcnblocks": "workspace:*",
    "@mdxui/tailark": "workspace:*",
    "@mdxui/tremor": "workspace:*",
    "@mdxui/mantine": "workspace:*",
    "mdxld": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  }
}
