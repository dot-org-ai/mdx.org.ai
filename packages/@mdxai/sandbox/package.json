    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.0",
    "wrangler": "^3.0.0"
  },
  "keywords": [
    "mdx",
    "ai",
    "claude",
    "sandbox",
    "cloudflare"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT"
}
