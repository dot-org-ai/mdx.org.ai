{
  "name": "@mdxai/service",
  "version": "0.0.0",
  "description": "Agent session platform with MDX-first progressive UI - Cloudflare Worker with Durable Objects",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./session-do": {
      "types": "./dist/session-do.d.ts",
      "import": "./dist/session-do.js",
      "require": "./dist/session-do.cjs"
    }
  },
  "files": [
    "dist",
    "wrangler.toml"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxai",
    "agent",
    "claude",
    "session",
    "durable-objects",
    "workers",
    "mdx",
    "websocket"
  ],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "hono": "^4.6.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@mdxld/config": "workspace:^",
    "@vitest/coverage-v8": "2.1.9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^4.21.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxai/service"
  },
  "author": "MDX.org.ai"
}
