{
  "name": "@mdxai/mastra",
  "version": "1.9.0",
  "description": "Mastra integration for mdxai",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxai",
    "mastra",
    "ai",
    "framework"
  ],
  "license": "MIT",
  "dependencies": {
    "@mdxdb/fs": "workspace:^",
    "mdxld": "workspace:^"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@mastra/core": "^0.24.6",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@mastra/core": "^0.24.0"
  },
  "peerDependenciesMeta": {
    "@mastra/core": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxai/mastra"
  },
  "author": "MDX.org.ai"
}
