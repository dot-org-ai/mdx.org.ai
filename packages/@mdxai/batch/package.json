{
  "name": "@mdxai/batch",
  "version": "1.0.0",
  "description": "Shared types and utilities for AI batch processing - unified interface for OpenAI, Claude, Google, Bedrock, and Cloudflare batch APIs",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxai",
    "batch",
    "ai",
    "openai",
    "claude",
    "anthropic",
    "google",
    "bedrock",
    "cloudflare"
  ],
  "license": "MIT",
  "dependencies": {
    "ai-database": "workspace:*"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxai/batch"
  },
  "author": "MDX.org.ai"
}
