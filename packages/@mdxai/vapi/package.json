{
  "name": "@mdxai/vapi",
  "version": "1.9.0",
  "description": "Vapi voice AI integration for mdxai",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxai",
    "vapi",
    "voice",
    "ai"
  ],
  "license": "MIT",
  "dependencies": {
    "@vapi-ai/server-sdk": "^0.10.2",
    "@vapi-ai/web": "^2.5.2",
    "mdxld": "workspace:^",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxai/vapi"
  },
  "author": "MDX.org.ai"
}
