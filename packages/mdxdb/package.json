{
  "name": "mdxdb",
  "version": "0.0.0",
  "description": "Create, Manage, & Publish MDX & URL-centric File System & Database",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mdxdb": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:exe": "bun scripts/build-exe.ts",
    "build:exe:current": "bun scripts/build-exe.ts --current",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist dist-exe .turbo"
  },
  "keywords": [
    "mdx",
    "mdxdb",
    "database",
    "filesystem",
    "api",
    "client"
  ],
  "license": "MIT",
  "dependencies": {
    "@mdxld/extract": "workspace:*",
    "ai-database": "workspace:*",
    "oauth.do": "^0.1.4"
  },
  "peerDependencies": {
    "mdxld": "workspace:*"
  },
  "devDependencies": {
    "@mdxdb/clickhouse": "workspace:*",
    "@mdxdb/studio": "workspace:*",
    "@mdxui/editor": "workspace:*",
    "mdxld": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "optionalDependencies": {
    "@mdxdb/studio": "workspace:*"
  }
}
