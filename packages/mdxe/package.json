{
  "name": "mdxe",
  "version": "1.9.1",
  "description": "Execute, Test, & Deploy MDX-based Agents, Apps, APIs, and Sites",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mdxe": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./sdk": {
      "types": "./dist/sdk.d.ts",
      "import": "./dist/sdk.js",
      "require": "./dist/sdk.cjs"
    },
    "./deploy": {
      "types": "./dist/commands/deploy.d.ts",
      "import": "./dist/commands/deploy.js",
      "require": "./dist/commands/deploy.cjs"
    },
    "./cloudflare": {
      "types": "./dist/cloudflare/api.d.ts",
      "import": "./dist/cloudflare/api.js",
      "require": "./dist/cloudflare/api.cjs"
    },
    "./tail": {
      "types": "./dist/tail/index.d.ts",
      "import": "./dist/tail/index.js",
      "require": "./dist/tail/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:exe": "bun scripts/build-exe.ts",
    "build:exe:current": "bun scripts/build-exe.ts --current",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist dist-exe .turbo"
  },
  "keywords": [
    "mdx",
    "mdxe",
    "execute",
    "deploy",
    "do",
    "cloudflare",
    "workers",
    "fumadocs",
    "opennext"
  ],
  "license": "MIT",
  "peerDependencies": {
    "mdxld": "^1.9.0"
  },
  "dependencies": {
    "@mdxdb/clickhouse": "^1.9.0",
    "@mdxdb/fs": "^1.9.0",
    "@mdxe/deploy": "^1.9.0",
    "@mdxe/fumadocs": "^0.0.1",
    "@mdxe/hono": "^1.9.0",
    "@mdxe/payload": "^1.9.0",
    "ai-database": "^0.2.0",
    "ai-functions": "^0.4.0",
    "ai-evaluate": "^0.1.0",
    "ai-workflows": "^2.0.0",
    "esbuild": "^0.27.0",
    "glob": "^10.4.5",
    "hono": "^4.6.0",
    "oauth.do": "^0.1.4",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.9",
    "mdxld": "^1.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/mdxe"
  },
  "author": "MDX.org.ai"
}
