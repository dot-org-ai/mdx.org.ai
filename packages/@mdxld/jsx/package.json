{
  "name": "@mdxld/jsx",
  "version": "1.9.0",
  "description": "MDX compilation and build tooling - tsup/vite/esbuild plugins for MDX files",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./dom": {
      "types": "./dist/dom.d.ts",
      "import": "./dist/dom.js",
      "require": "./dist/dom.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxld",
    "mdx",
    "jsx",
    "tsup",
    "esbuild",
    "vite",
    "plugin",
    "compile"
  ],
  "license": "MIT",
  "dependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "gray-matter": "^4.0.3"
  },
  "peerDependencies": {
    "esbuild": ">=0.17.0",
    "hono": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "esbuild": "^0.24.0",
    "hono": "^4.6.0",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxld/jsx"
  },
  "author": "MDX.org.ai"
}
