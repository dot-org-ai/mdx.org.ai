{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "declaration": false,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "out/test",
    "rootDir": "src/test",
    "lib": ["ES2022"]
  },
  "include": ["src/test/**/*"],
  "exclude": ["node_modules"]
}
