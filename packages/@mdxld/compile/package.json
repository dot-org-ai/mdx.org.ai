{
  "name": "@mdxld/compile",
  "version": "1.9.0",
  "description": "Compile MDX/JSX to executable JavaScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "esbuild": "^0.24.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "mdxld": "workspace:*"
  },
  "peerDependenciesMeta": {
    "mdxld": {
      "optional": true
    }
  },
  "keywords": [
    "mdx",
    "mdxld",
    "compile",
    "jsx",
    "transform"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxld/compile"
  },
  "author": "MDX.org.ai"
}
