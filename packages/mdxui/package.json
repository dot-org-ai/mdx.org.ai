{
  "name": "mdxui",
  "version": "1.9.0",
  "description": "Compose, Design, & Publish MDX-based UI Components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mdxui": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./products": {
      "types": "./dist/products.d.ts",
      "import": "./dist/products.js",
      "require": "./dist/products.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdx",
    "mdxui",
    "ui",
    "components",
    "storybook"
  ],
  "license": "MIT",
  "devDependencies": {
    "@mdxui/storybook": "workspace:^",
    "digital-products": "workspace:^",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@mdxui/storybook": "workspace:*",
    "digital-products": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@mdxui/storybook": {
      "optional": true
    },
    "digital-products": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/mdxui"
  },
  "author": "MDX.org.ai"
}
