{
  "name": "@mdxe/hono",
  "version": "1.9.0",
  "description": "Serve and render MDXLD documents with Hono",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./jsx": {
      "types": "./dist/jsx.d.ts",
      "import": "./dist/jsx.js",
      "require": "./dist/jsx.cjs"
    },
    "./fumadocs": {
      "types": "./dist/fumadocs.css.d.ts",
      "import": "./dist/fumadocs.css.js",
      "require": "./dist/fumadocs.css.cjs"
    },
    "./templates": {
      "types": "./dist/templates.d.ts",
      "import": "./dist/templates.js",
      "require": "./dist/templates.cjs"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "import": "./dist/styles.js",
      "require": "./dist/styles.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxe",
    "mdxld",
    "hono",
    "web",
    "server",
    "edge"
  ],
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.13.0",
    "@mdxui/fumadocs": "workspace:*",
    "mdxld": "workspace:*"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "hono": "^4.6.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxe/hono"
  },
  "author": "MDX.org.ai"
}
