{
  "name": "@mdxe/honox",
  "version": "1.9.0",
  "description": "HonoX integration for MDXLD - file-based routing, islands, and MDX with linked data",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./renderer": {
      "types": "./dist/renderer.d.ts",
      "import": "./dist/renderer.js",
      "require": "./dist/renderer.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxe",
    "mdxld",
    "honox",
    "hono",
    "vite",
    "islands",
    "ssr",
    "file-based-routing",
    "linked-data",
    "json-ld"
  ],
  "license": "MIT",
  "dependencies": {
    "@mdxld/jsx": "workspace:*",
    "mdxld": "workspace:*",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.0.0"
  },
  "peerDependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "hono": "^4.0.0",
    "honox": "^0.1.0",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "@mdx-js/rollup": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.1.0",
    "@types/node": "^22.10.1",
    "hono": "^4.6.0",
    "honox": "^0.1.31",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxe/honox"
  },
  "author": "MDX.org.ai"
}
