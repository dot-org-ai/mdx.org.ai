{
  "name": "@mdxe/deploy",
  "version": "1.9.1",
  "description": "Unified deployment interface for MDX projects - deploy to .do, Cloudflare, Vercel, or GitHub Pages",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxe",
    "deploy",
    "do",
    "cloudflare",
    "vercel",
    "github",
    "github-pages",
    "mdx"
  ],
  "license": "MIT",
  "dependencies": {
    "@mdxe/do": "^1.9.0",
    "@mdxe/cloudflare": "^1.9.0",
    "@mdxe/github": "^1.9.0",
    "@mdxe/vercel": "^1.9.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxe/deploy"
  },
  "author": "MDX.org.ai"
}
