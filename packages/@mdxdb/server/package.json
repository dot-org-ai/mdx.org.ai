{
  "name": "@mdxdb/server",
  "version": "0.0.0",
  "description": "HTTP API server for mdxdb using Hono - works with any runtime",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./db": {
      "types": "./dist/db-server.d.ts",
      "import": "./dist/db-server.js",
      "require": "./dist/db-server.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "server",
    "http",
    "rest",
    "hono",
    "api"
  ],
  "license": "MIT",
  "dependencies": {
    "hono": "^4.6.0"
  },
  "peerDependencies": {
    "ai-database": "workspace:*",
    "mdxdb": "workspace:*",
    "mdxld": "workspace:*"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@mdxdb/fs": "workspace:*",
    "ai-database": "workspace:*",
    "mdxdb": "workspace:*",
    "mdxld": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxdb/server"
  },
  "author": "MDX.org.ai"
}
