{
  "name": "@mdxdb/github",
  "version": "0.0.0",
  "description": "GitHub adapter for mdxdb - stores MDX documents in GitHub repositories via Octokit",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --exclude tests/integration.test.ts",
    "test:unit": "vitest run --exclude tests/integration.test.ts",
    "test:integration": "vitest run tests/integration.test.ts",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "github",
    "octokit",
    "adapter",
    "mdx",
    "database"
  ],
  "license": "MIT",
  "dependencies": {
    "octokit": "^4.0.2"
  },
  "peerDependencies": {
    "mdxdb": "workspace:*",
    "mdxld": "workspace:*"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "mdxdb": "workspace:*",
    "mdxld": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxdb/github"
  },
  "author": "MDX.org.ai"
}
