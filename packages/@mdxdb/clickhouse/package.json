{
  "name": "@mdxdb/clickhouse",
  "version": "0.0.0",
  "description": "ClickHouse adapter for mdxdb - optimized for analytics and event sourcing",
  "type": "module",
  "main": "./dist/src/index.cjs",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js",
      "require": "./dist/schema/index.cjs"
    },
    "./schema/migrate": {
      "types": "./dist/schema/migrate.d.ts",
      "import": "./dist/schema/migrate.js",
      "require": "./dist/schema/migrate.cjs"
    },
    "./sync": {
      "types": "./dist/sync/index.d.ts",
      "import": "./dist/sync/index.js",
      "require": "./dist/sync/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "clickhouse",
    "chdb",
    "analytics",
    "database",
    "adapter",
    "event-sourcing",
    "cloudflare-workers",
    "git-sync",
    "bi-directional-sync"
  ],
  "license": "MIT",
  "dependencies": {
    "mdxdb": "workspace:^"
  },
  "peerDependencies": {
    "chdb": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "chdb": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "chdb": "^1.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
