{
  "name": "@mdxdb/clickhouse",
  "version": "0.0.0",
  "description": "ClickHouse adapter for mdxdb - optimized for analytics and event sourcing",
  "type": "module",
  "main": "./dist/src/index.cjs",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.cjs"
    },
    "./worker": {
      "types": "./dist/src/worker.d.ts",
      "import": "./dist/src/worker.js",
      "require": "./dist/src/worker.cjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js",
      "require": "./dist/schema/index.cjs"
    },
    "./schema/migrate": {
      "types": "./dist/schema/migrate.d.ts",
      "import": "./dist/schema/migrate.js",
      "require": "./dist/schema/migrate.cjs"
    },
    "./sync": {
      "types": "./dist/sync/index.d.ts",
      "import": "./dist/sync/index.js",
      "require": "./dist/sync/index.cjs"
    },
    "./server": {
      "types": "./dist/src/server.d.ts",
      "import": "./dist/src/server.js",
      "require": "./dist/src/server.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "worker": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "worker:deploy:preview": "wrangler deploy --env preview",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "clickhouse",
    "analytics",
    "database",
    "adapter",
    "event-sourcing",
    "cloudflare-workers",
    "git-sync",
    "bi-directional-sync"
  ],
  "license": "MIT",
  "dependencies": {
    "ai-database": "workspace:^",
    "mdxdb": "workspace:^",
    "@mdxld/extract": "workspace:^"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@mdxld/config": "workspace:^",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wrangler": "^3.93.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxdb/clickhouse"
  },
  "author": "MDX.org.ai"
}
