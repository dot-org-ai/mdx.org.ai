{
  "name": "@mdxdb/parquet",
  "version": "1.0.0",
  "description": "Pure JS parquet read/write for mdxdb - works in Cloudflare Workers and Snippets",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./reader": {
      "types": "./dist/reader.d.ts",
      "import": "./dist/reader.js",
      "require": "./dist/reader.cjs"
    },
    "./writer": {
      "types": "./dist/writer.d.ts",
      "import": "./dist/writer.js",
      "require": "./dist/writer.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "parquet",
    "hyparquet",
    "cloudflare",
    "workers",
    "snippets",
    "columnar",
    "storage"
  ],
  "license": "MIT",
  "dependencies": {
    "hyparquet": "^1.7.1"
  },
  "optionalDependencies": {
    "hyparquet-compressors": "^1.1.4"
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxdb/parquet"
  },
  "author": "MDX.org.ai"
}
