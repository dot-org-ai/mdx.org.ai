{
  "name": "@mdxdb/api",
  "version": "0.0.0",
  "description": "HTTP API client for mdxdb - fetch-based REST client",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./db": {
      "types": "./dist/db-client.d.ts",
      "import": "./dist/db-client.js",
      "require": "./dist/db-client.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "api",
    "client",
    "http",
    "rest",
    "fetch"
  ],
  "license": "MIT",
  "peerDependencies": {
    "ai-database": "workspace:*",
    "mdxld": "workspace:*"
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.0",
    "@mdxdb/clickhouse": "workspace:*",
    "@mdxdb/server": "workspace:*",
    "@mdxld/config": "workspace:^",
    "ai-database": "workspace:*",
    "mdxld": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxdb/api"
  },
  "author": "MDX.org.ai"
}
