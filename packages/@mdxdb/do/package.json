{
  "name": "@mdxdb/do",
  "version": "1.0.0",
  "description": "Extended Durable Object with parent/child hierarchy, hibernatable WebSocket, and parquet export",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./durable-object": {
      "types": "./dist/durable-object.d.ts",
      "import": "./dist/durable-object.js",
      "require": "./dist/durable-object.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./hierarchy": {
      "types": "./dist/hierarchy.d.ts",
      "import": "./dist/hierarchy.js",
      "require": "./dist/hierarchy.cjs"
    },
    "./hibernatable-ws": {
      "types": "./dist/hibernatable-ws.d.ts",
      "import": "./dist/hibernatable-ws.js",
      "require": "./dist/hibernatable-ws.cjs"
    },
    "./parquet-export": {
      "types": "./dist/parquet-export.d.ts",
      "import": "./dist/parquet-export.js",
      "require": "./dist/parquet-export.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "durable-objects",
    "cloudflare",
    "workers",
    "hierarchy",
    "hibernatable",
    "websocket",
    "parquet",
    "rpc"
  ],
  "license": "MIT",
  "dependencies": {
    "@mdxdb/parquet": "workspace:^",
    "@mdxdb/sqlite": "workspace:^",
    "rpc.do": "^0.1.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@mdxld/config": "workspace:^",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-primitives/mdx.org.ai",
    "directory": "packages/@mdxdb/do"
  },
  "author": "MDX.org.ai"
}
