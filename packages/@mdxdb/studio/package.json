{
  "name": "@mdxdb/studio",
  "version": "0.0.0",
  "description": "MDXDB Studio - Full-featured MDX editor with database integration",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mdxdb-studio": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./electron": {
      "types": "./dist/electron.d.ts",
      "import": "./dist/electron.js",
      "require": "./dist/electron.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "mdxdb",
    "studio",
    "editor",
    "mdx",
    "database"
  ],
  "license": "MIT",
  "dependencies": {
    "mdxld": "workspace:*"
  },
  "peerDependencies": {
    "@mdxui/editor": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@mdxui/editor": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mdxld/config": "workspace:^",
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
